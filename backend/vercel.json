{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/send-otp.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/send-otp",
        "dest": "backend/api/send-otp.js"
      }
    ],
    "env": {
      "SMTP_USER": "@smtp_user",
      "SMTP_PASS": "@smtp_pass"
    }
  }
  